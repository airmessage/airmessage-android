<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
	<cloud-backup>
		<include domain="sharedpref" path="." />
		<exclude domain="sharedpref" path="connectivity.xml" />
		<exclude domain="sharedpref" path="secure.xml" />
	</cloud-backup>
	
	<device-transfer>
		<include domain="sharedpref" path="." />
		<exclude domain="sharedpref" path="connectivity.xml" />
		<exclude domain="sharedpref" path="secure.xml" />
	</device-transfer>
</data-extraction-rules>